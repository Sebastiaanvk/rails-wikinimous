<p id="notice"><%= notice %></p>





<body>
  <div class="content">

    <h1>Articles</h1>

    <%= link_to 'New Article', new_article_path %>
    <% @articles.each do |article| %>
    <div class="home_article_view">
      <div><%= link_to article.title, article %></div>
      <div><%= article.content %></div>
      <div><%= link_to 'Edit', edit_article_path(article) %></div>
      <div><%= link_to 'Destroy', article, method: :delete, data: { confirm: 'Are you sure?' } %></div>
    </div>
    <% end %>
  </div>
</body>


<br>

